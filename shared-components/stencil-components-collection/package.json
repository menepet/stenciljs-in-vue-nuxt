{
  "name": "stencil-components-collection",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components-collection/stencil-components-collection.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "node_modules/.bin/stencil build --docs",
    "build-static": "node_modules/.bin/stencil build --prerender",
    "start": "node_modules/.bin/stencil build --watch --serve",
    "test": "node_modules/.bin/stencil test --spec --e2e",
    "test.watch": "node_modules/.bin/stencil test --spec --e2e --watchAll",
    "generate": "node_modules/.bin/stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.3",
    "workbox-build": "^5.1.4"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/sass": "^1.3.2"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.0"
  }
}
